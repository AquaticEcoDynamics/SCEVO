	<SensorScript>
		<name>Net Radiation</name>
		<longname>Net Radiation</longname>
		<SensorInfo>
			<title>Net Radiation</title>
			<units>W m-2</units>
		</SensorInfo>
		<ScriptBlock>
			<ScriptStream>
				<name>downLW</name>
				<address>$PARENT.GLW</address>
			</ScriptStream>
			<ScriptStream>
				<name>outLW</name>
				<address>$PARENT.OLR</address>
			</ScriptStream>
			<ScriptStream>
				<name>downSW</name>
				<address>$PARENT.SWDOWN</address>
			</ScriptStream>
			<ScriptStream>
				<name>albedo</name>
				<address>$PARENT.ALBEDO</address>
			</ScriptStream>
			
			<script>
				(downLW + downSW)-(outLW + (downSW * albedo));
			</script>
		</ScriptBlock>
		<PlotProperties>
			<Limits>
				<yLimits>[-500 1000]</yLimits>
			</Limits>
		</PlotProperties>
	</SensorScript>
	<SensorScript>
		<name>Net LW Radiation</name>
		<longname>Net Long Wave Radiation</longname>
		<SensorInfo>
			<title>Net LW Radiation</title>
			<units>W m-2</units>
		</SensorInfo>
		<ScriptBlock>
			<ScriptStream>
				<name>downLW</name>
				<address>$PARENT.GLW</address>
			</ScriptStream>
			<ScriptStream>
				<name>outLW</name>
				<address>$PARENT.OLR</address>
			</ScriptStream>
			
			<script>
				downLW - outLW;	
			</script>
		</ScriptBlock>
		<PlotProperties>
			<Limits>
				<yLimits>[-200 800]</yLimits>
			</Limits>
		</PlotProperties>
	</SensorScript>
	<SensorScript>
		<name>Net SW Radiation</name>
		<longname>Net Short Wave Radiation</longname>
		<SensorInfo>
			<title>Net SW Radiation</title>
			<units>W m-2</units>
		</SensorInfo>
		<ScriptBlock>
			<ScriptStream>
				<name>downSW</name>
				<address>$PARENT.SWDOWN</address>
			</ScriptStream>
			<ScriptStream>
				<name>albedo</name>
				<address>$PARENT.ALBEDO</address>
			</ScriptStream>
			
			<script>
				downSW - (downSW * albedo);
			</script>
		</ScriptBlock>
		<PlotProperties>
			<Limits>
				<yLimits>[0 1400]</yLimits>
			</Limits>
		</PlotProperties>
	</SensorScript>
	<SensorScript>
		<name>RELHUMFACTOR</name>
		<longname>Relative Humidity - Factor</longname>
		<SensorInfo>
			<title>Relative Humidity</title>
			<units>Factor</units>
		</SensorInfo>
		<ScriptBlock>
			<ScriptStream>
				<name>relhum</name>
				<address>$PARENT.RELHUM</address>
			</ScriptStream>
			
			<script>
				relhum * 0.01;
			</script>
		</ScriptBlock>
		<PlotProperties>
			<Limits>
				<yLimits>[0 1]</yLimits>
			</Limits>
		</PlotProperties>
	</SensorScript>
</WRFVariableFile>