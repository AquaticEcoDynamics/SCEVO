	<SensorScript>
		<name>Surface Pressure (hPa)</name>
		<longname>Surface Pressure (hPa)</longname>
		<SensorInfo>
			<title>Surface Pressure (hPa)</title>
			<units>hPa</units>
		</SensorInfo>
		<ScriptBlock>
			<process2DUsingScript>true</process2DUsingScript>
			<ScriptStream>
				<name>pressure</name>
				<address>$PARENT.PSFC</address>
			</ScriptStream>
			
			<script>
				var i, j;

				for (i=0; i &lt; result.length; i++) {
					for (j=0; j &lt; result[0].length; j++) {
						result[i][j] = pressure[i][j] / 100.0;
					}
				}
			</script>
		</ScriptBlock>
		<PlotProperties>
			<Limits>
				<colorLimits>[700 1200]</colorLimits>
			</Limits>
		</PlotProperties>
	</SensorScript>
	<SensorScript>
		<name>Net Radiation</name>
		<longname>Net Radiation</longname>
		<SensorInfo>
			<title>Net Radiation</title>
			<units>W m-2</units>
		</SensorInfo>
		<ScriptBlock>
			<process2DUsingScript>true</process2DUsingScript>
			<ScriptStream>
				<name>downLW</name>
				<address>$PARENT.GLW</address>
			</ScriptStream>
			<ScriptStream>
				<name>outLW</name>
				<address>$PARENT.OLR</address>
			</ScriptStream>
			<ScriptStream>
				<name>downSW</name>
				<address>$PARENT.SWDOWN</address>
			</ScriptStream>
			<ScriptStream>
				<name>albedo</name>
				<address>$PARENT.ALBEDO</address>
			</ScriptStream>
			
			<script>
				var i, j;

				for (i=0; i &lt; result.length; i++) {
					for (j=0; j &lt; result[0].length; j++) {
						result[i][j] = (downLW[i][j] + downSW[i][j]) - (outLW[i][j] + (downSW[i][j] * albedo[i][j]));
					}
				}
			</script>
		</ScriptBlock>
		<PlotProperties>
			<Limits>
				<colorLimits>[-500 1000]</colorLimits>
				<contourLevels>[-500:40:1000]</contourLevels>
			</Limits>
		</PlotProperties>
	</SensorScript>
	<SensorScript>
		<name>Net LW Radiation</name>
		<longname>Net Long Wave Radiation</longname>
		<SensorInfo>
			<title>Net LW Radiation</title>
			<units>W m-2</units>
		</SensorInfo>
		<ScriptBlock>
			<process2DUsingScript>true</process2DUsingScript>
			<ScriptStream>
				<name>downLW</name>
				<address>$PARENT.GLW</address>
			</ScriptStream>
			<ScriptStream>
				<name>outLW</name>
				<address>$PARENT.OLR</address>
			</ScriptStream>
			
			<script>
				var i, j;

				for (i=0; i &lt; result.length; i++) {
					for (j=0; j &lt; result[0].length; j++) {
						result[i][j] = downLW[i][j] - outLW[i][j];
					}
				}
			</script>
		</ScriptBlock>
		<PlotProperties>
			<Limits>
				<colorLimits>[-200 800]</colorLimits>
				<contourLevels>[-200:40:800]</contourLevels>
			</Limits>
		</PlotProperties>
	</SensorScript>
	<SensorScript>
		<name>Net SW Radiation</name>
