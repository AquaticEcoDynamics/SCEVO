			</ScriptStream>
			
			<script>
				var i, j;

				for (i=0; i &lt; result.length; i++) {
					for (j=0; j &lt; result[0].length; j++) {
						result[i][j] = downSW[i][j] - (downSW[i][j] * albedo[i][j]);
					}
				}
			</script>
		</ScriptBlock>
		<PlotProperties>
			<Limits>
				<colorLimits>[0 1400]</colorLimits>
				<contourLevels>[0:50:1400]</contourLevels>
			</Limits>
		</PlotProperties>
	</SensorScript>
