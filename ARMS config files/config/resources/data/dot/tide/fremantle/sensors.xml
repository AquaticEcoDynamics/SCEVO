<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SensorFile>
	<comment>DOT - Fremantle</comment>
	<Sensor1D>
		<name>tide</name>
		<longname>Tide</longname>
		<SensorInfo>
			<title>Tide (cm)</title>
			<units>cm</units>
			<FileInfo>
				<fileColumn>1</fileColumn>
			</FileInfo>
			<Location>
				<datum>LWM</datum>
				<zOffset>0.0</zOffset>
			</Location>
			<Limits>
				<minValue>20</minValue>
				<maxValue>150</maxValue>
			</Limits>
			<Defaults>
				<defaultValue>80</defaultValue>
			</Defaults>
		</SensorInfo>
		<PlotProperties>
			
		</PlotProperties>
	</Sensor1D>
	<SensorScript>
		<name>tide(m)</name>
		<longname>Tide(m)</longname>
		<SensorInfo>
			<title>Tide(m)</title>
			<units>m</units>
		</SensorInfo>
		<ScriptBlock>
			<ScriptStream>
				<name>tide</name>
				<address>$PARENT.tide</address>		
			</ScriptStream>
			<script>
				tide*0.10;
			</script>
		</ScriptBlock>
		<PlotProperties>
			<Limits>
				<yLimits>[0.2 1.5]</yLimits>
			</Limits>
		</PlotProperties>
	</SensorScript>
</SensorFile>
