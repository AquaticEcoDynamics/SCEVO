<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SensorFile>

	<comment>Matild Buoy weather unit (Gill GMX600)</comment>

	<Sensor1D>
		<name>air_temperature</name>
		<longname>Air Temperature</longname>
		<SensorInfo>
			<title>Air Temperature</title>
			<units>deg C</units>
			<FileInfo>
				<fileColumn>3</fileColumn>
			</FileInfo>
			<Location>
				<datum>ESTUARY_SURFACE</datum>
				<zOffset>+0.5</zOffset>
			</Location>
			<Limits>
				<minValue>-50</minValue>
				<maxValue>60</maxValue>
			</Limits>
			<Defaults>
				<defaultValue>15</defaultValue>
			</Defaults>
		</SensorInfo>
		<PlotProperties>
			
		</PlotProperties>
	</Sensor1D>
	<Sensor1D>
		<name>baro_pressure</name>
