		<SensorInfo>
			<title>Atmospheric Pressure</title>
			<units>hPa</units>
			<FileInfo>
				<fileColumn>4</fileColumn>
			</FileInfo>
			<Location>
				<datum>ESTUARY_SURFACE</datum>
				<zOffset>+0.5</zOffset>
			</Location>
			<Limits>
				<minValue>800</minValue>
				<maxValue>2000</maxValue>
			</Limits>
			<Defaults>
				<defaultValue>1013</defaultValue>
