<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SensorFile>

	<comment>Matilda Buoy Logger (Campbell)</comment>

	<Sensor1D>
		<name>battery</name>
		<longname>Main Battery Voltage</longname>
		<SensorInfo>
			<title>Main Battery</title>
			<units>V</units>
			<FileInfo>
				<fileColumn>3</fileColumn>
			</FileInfo>
			<Location>
				<datum>LWM</datum>
				<zOffset>0.0</zOffset>
			</Location>
			<Limits>
				<minValue>0</minValue>
				<maxValue>50</maxValue>
			</Limits>
			<Defaults>
				<defaultValue>0</defaultValue>
			</Defaults>
		</SensorInfo>
		<PlotProperties>
			
		</PlotProperties>
	</Sensor1D>
	
	<Sensor1D>
		<name>system_temperature</name>
		<longname>System Temperature</longname>
		<SensorInfo>
			<title>System Temperature</title>
			<units>deg C</units>
			<FileInfo>
				<fileColumn>4</fileColumn>
			</FileInfo>
			<Location>
				<datum>LWM</datum>
				<zOffset>0.0</zOffset>
			</Location>
			<Limits>
				<minValue>-50</minValue>
				<maxValue>80</maxValue>
			</Limits>
			<Defaults>
				<defaultValue>0</defaultValue>
			</Defaults>
		</SensorInfo>
		<PlotProperties>
			
		</PlotProperties>
	</Sensor1D>

</SensorFile>
