<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SensorFile>
	<comment>Ocean Reef Station derived sensor. Wind speed m/s</comment>
	<SensorScript>
		<name>wind_speed_m_s</name>
		<longname>Wind Speed (m/s)</longname>
		<SensorInfo>
			<title>Wind Speed (m/s)</title>
			<units>m/s</units>
		</SensorInfo>
		<ScriptBlock>
			<ScriptStream>
				<name>windspeed</name>
				<address>$PARENT.windspeed</address>		
			</ScriptStream>
			<script>
				windspeed * 0.514444;
			</script>
		</ScriptBlock>
		<PlotProperties>
			<Limits>
				<yLimits>[0 15]</yLimits>
			</Limits>
		</PlotProperties>
	</SensorScript>
</SensorFile>
