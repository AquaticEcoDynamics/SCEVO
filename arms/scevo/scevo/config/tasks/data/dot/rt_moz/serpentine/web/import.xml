<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TextFileImport>
	<comment>Import Serpentine Tide data</comment>
	<localFolder>data/dot/rt_moz/serpentine</localFolder>
	<fileName>moz_serpentine*.csv</fileName>
	<setImportedFlag>true</setImportedFlag>
	<importLastFile>false</importLastFile>
	<startDate>END_DATE@scevo.data.dot.rt_moz.serpentine.web.water_level</startDate>
	<timeZone>+8.0</timeZone>
	<destinationAddress>scevo.data.dot.rt_moz.serpentine.web</destinationAddress>
	<dateFormat>dd/MM/yyyy HH:mm</dateFormat> <!-- 08/12/2021 00:30,1.17,9,1.08 -->
	<nHeaderLines>1</nHeaderLines>
	<nTailLines>0</nTailLines>
	<continueOnError>false</continueOnError>
	<separator>,</separator>
	<dateColumn>1</dateColumn>
	<!-- <datum>AHD</datum> -->
	<sensorsFile>config/resources/data/dot/rt_moz/serpentine/sensors.xml</sensorsFile>
</TextFileImport>
