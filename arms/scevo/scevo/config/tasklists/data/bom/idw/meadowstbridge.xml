<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TaskListConfig>
	<!-- To Stop tasks put
	<TaskConfig>
		<taskName>stop</taskName>
		<configFile></configFile>
	</TaskConfig>
	-->
	
	
	

	<taskRootDir>config/tasks/data/bom/idw/meadowstbridge</taskRootDir>
	<skipNTasks>0</skipNTasks>

    <!-- Move DBCA files downloaded from the web -->
    <TaskConfig>
        <taskName>Copy Files</taskName>
        <configFile>copy_idw_file.xml</configFile>
    </TaskConfig>


	<!-- Import to DB -->     
	<TaskConfig>
		<taskName>Text File Import</taskName>
		<configFile>import.xml</configFile>
		<continueOnError>true</continueOnError>
	</TaskConfig>

    <!-- Delete files in the web download folder -->	
    <TaskConfig>
        <taskName>Delete Files</taskName>
        <configFile>delete_idw_web_dl.xml</configFile>
    </TaskConfig> 

	
</TaskListConfig>

