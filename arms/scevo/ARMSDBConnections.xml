<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<JDBCList>
  <JDBC>
    <name>SCEVO Write Access</name>
    <driver>org.postgresql.Driver</driver>
    <url>jdbc:postgresql://localhost/scevo</url>
    <database>scevo</database>
    <ARMSDataBase>true</ARMSDataBase>
    <loadOnStartUp>true</loadOnStartUp>
    <PasswordLookup>SCEVO_DB_WRITE</PasswordLookup>
    <armsGUIFile>arms_gui.xml</armsGUIFile>
    <configRootDir>config</configRootDir>
    <modelRootDir>models</modelRootDir>
    <taskSchedulerListFile>config/services/scheduler.xml</taskSchedulerListFile>
    <startSchedulerOnStartUp>true</startSchedulerOnStartUp>
    <maxActiveConnections>5</maxActiveConnections>
    <maxWaitForConnection>5000</maxWaitForConnection>
    <testpi>false</testpi>
  </JDBC>
</JDBCList>
